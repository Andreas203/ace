    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.0.3",
    "three": "^0.146.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
